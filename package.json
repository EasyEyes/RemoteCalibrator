{
  "name": "remote-calibrator",
  "version": "0.0.7",
  "description": "A toolbox for remote testing calibration and tracking.",
  "main": "lib/RemoteCalibrator.min.js",
  "directories": {
    "lib": "lib",
    "homepage": "homepage"
  },
  "scripts": {
    "setup": "cd src/WebGazer4RC && npm i && cd ../.. && npm i",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "start": "webpack --env=development",
    "dev": "webpack --env=development",
    "build": "webpack --env=production",
    "serve": "node server.js",
    "prepare": "husky install",
    "netlify": "npm run setup && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyEyes/remote-calibrator.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EasyEyes/remote-calibrator/issues"
  },
  "homepage": "https://github.com/EasyEyes/remote-calibrator#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.27.0",
    "eslint-webpack-plugin": "^2.5.4",
    "express": "^4.17.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "sass-loader": "^11.1.1",
    "style-loader": "^3.1.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.0",
    "webpack-modules": "^1.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css}": "prettier --write"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "device-detector-js": "^2.2.10",
    "platform": "^1.3.6",
    "sweetalert2": "^11.0.18"
  }
