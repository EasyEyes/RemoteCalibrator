{
  "name": "remote-calibrator",
  "version": "0.6.3-beta.1",
  "description": "Useful calibration tools for remote psychophysics experiments.",
  "main": "lib/RemoteCalibrator.min.js",
  "directories": {
    "lib": "lib",
    "homepage": "homepage"
  },
  "scripts": {
    "setup": "cd src/WebGazer4RC && git checkout rc-prod && npm i && cd ../.. && npm i",
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss}\"",
    "start": "webpack serve --env=development",
    "build": "webpack --env=production",
    "prepare": "husky install",
    "netlify": "cp -r ./lib ./homepage",
    "phrases": "node i18n/fetch-languages-sheets.js && git add src/i18n.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyEyes/remote-calibrator.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EasyEyes/remote-calibrator/issues"
  },
  "homepage": "https://calibrator.app",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.12",
    "d3": "^7.4.5",
    "eslint": "^8.18.0",
    "eslint-webpack-plugin": "^3.2.0",
    "googleapis": "^104.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.1",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-modules": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css,scss}": "prettier --write"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "leader-line-new": "^1.1.9",
    "platform": "^1.3.6",
    "react-fast-compare": "^3.2.0",
    "sweetalert2": "^11.4.18",
    "tinycolor2": "^1.4.2",
    "tone": "^14.7.77"
