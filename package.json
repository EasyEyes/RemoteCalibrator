{
  "name": "remote-calibrator",
  "version": "0.7.5",
  "description": "Useful calibration tools for remote psychophysics experiments.",
  "main": "lib/RemoteCalibrator.min.js",
  "directories": {
    "lib": "lib",
    "homepage": "homepage"
  },
  "scripts": {
    "setup": "git submodule update --init --recursive && cd src/WebGazer4RC && npm i --force && cd ../.. && npm i",
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss}\"",
    "start": "webpack serve --env=development",
    "build": "webpack --env=production",
    "test": "webpack --env mocha --config=test/webpack.config.test.js && mocha test-exec/test.min.js",
    "prepare": "husky install",
    "netlify": "cp -r ./lib ./homepage",
    "phrases": "node i18n/fetch-languages-sheets.js && git add src/i18n.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyEyes/remote-calibrator.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EasyEyes/remote-calibrator/issues"
  },
  "homepage": "https://calibrator.app",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "bufferutil": "^4.0.8",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.1",
    "eslint": "^9.3.0",
    "eslint-webpack-plugin": "^4.2.0",
    "googleapis": "^137.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "leader-line-new": "^1.1.9",
    "lint-staged": "^15.2.5",
    "mocha": "^10.4.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "platform": "^1.3.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "react-fast-compare": "^3.2.2",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "sweetalert2": "^11.11.0",
    "terser-webpack-plugin": "^5.3.10",
    "tinycolor2": "^1.6.0",
    "tone": "14.7.77",
    "url-loader": "^4.1.1",
    "utf-8-validate": "^6.0.4",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-modules": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css,scss}": "prettier --write"
  },
  "files": [
    "lib",
    "CHANGELOG.md"
  ]
}
