{
  "name": "remote-calibrator",
  "version": "0.0.1",
  "description": "A toolbox for remote testing calibration.",
  "main": "lib/main.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "scripts": {
    "setup": "npm i && cd src/WebGazer && npm i && npm run build && cd ../..",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "dev": "webpack --env=development",
    "build": "webpack --env=production",
    "serve": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peilingjiang/RemoteCalibrator.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peilingjiang/RemoteCalibrator/issues"
  },
  "homepage": "https://github.com/peilingjiang/RemoteCalibrator#readme",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.2",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.5.0",
    "webpack-modules": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add lib && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,html,json}": "prettier --write"
  },
  "dependencies": {
    "platform": "^1.3.6"
  }
}
