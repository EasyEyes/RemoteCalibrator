{
  "name": "remote-calibrator",
  "version": "0.5.0-beta.0",
  "description": "Useful calibration tools for remote psychophysics experiments.",
  "main": "lib/RemoteCalibrator.min.js",
  "directories": {
    "lib": "lib",
    "homepage": "homepage"
  },
  "scripts": {
    "setup": "cd src/WebGazer4RC && npm i && cd ../.. && npm i",
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss}\"",
    "start": "webpack --env=development",
    "dev": "webpack --env=development",
    "build": "webpack --env=production",
    "serve": "node server.js",
    "prepare": "husky install",
    "netlify": "cp -r ./lib ./homepage",
    "phrases": "node i18n/fetch-languages-sheets.js && git add src/i18n.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyEyes/remote-calibrator.git"
  },
  "author": "Peiling Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EasyEyes/remote-calibrator/issues"
  },
  "homepage": "https://github.com/EasyEyes/remote-calibrator#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.0.1",
    "express": "^4.17.1",
    "googleapis": "^88.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.13",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-modules": "^1.0.0",
    "xlsx": "^0.17.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css,scss}": "prettier --write"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "device-detector-js": "^2.2.10",
    "platform": "^1.3.6",
    "react-fast-compare": "^3.2.0",
    "sweetalert2": "^11.1.8",
    "tinycolor2": "^1.4.2",
    "tone": "^14.7.77"
  }
