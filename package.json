  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html,css,scss}": "prettier --write"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "device-detector-js": "^3.0.2",
    "platform": "^1.3.6",
